{
  "name": "next-enterprise",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env FORCE_COLOR=1 next dev --port 3030",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,mdx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx,mdx}\" --fix",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "format": "yarn lint:fix && yarn prettier:fix",
    "analyze": "cross-env ANALYZE=true yarn build",
    "test": "cross-env FORCE_COLOR=1 jest --passWithNoTests --silent",
    "e2e:headless": "cross-env ANALYZE=false playwright test",
    "e2e:ui": "cross-env ANALYZE=false playwright test --ui",
    "types:check": "tsc --noEmit",
    "types:create": "DOTENV_CONFIG_PATH=.env graphql-codegen -r dotenv/config",
    "postinstall": "npx patch-package -y",
    "preinstall": "npx only-allow yarn",
    "source-icons": "node lib/gen-sprite.mjs -s src/assets/svg -o public/sprite.svg"
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@apollo/experimental-nextjs-app-support": "^0.1.0",
    "@radix-ui/react-accordion": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.4",
    "@radix-ui/react-form": "^0.0.2",
    "@radix-ui/react-label": "^2.0.1",
    "@radix-ui/react-popover": "^1.0.5",
    "@radix-ui/react-radio-group": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.0.3",
    "@radix-ui/react-select": "1.2.1",
    "@radix-ui/react-slider": "^1.1.1",
    "@radix-ui/react-switch": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^10.0.3",
    "@semantic-release/release-notes-generator": "^11.0.1",
    "class-variance-authority": "^0.6.0",
    "graphql": "^16.6.0",
    "lint-staged": "^13.2.2",
    "next": "^13.4.4",
    "react": "^18.2.0",
    "react-datocms": "^4.0.15",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@graphql-codegen/cli": "^4.0.0",
    "@graphql-codegen/client-preset": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-graphql-request": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@jest/globals": "^29.5.0",
    "@next/bundle-analyzer": "^13.3.0",
    "@playwright/test": "^1.32.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/node": "^18.0.0",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.54.1",
    "all-contributors-cli": "^6.24.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "eslint": "^8.41.0",
    "eslint-config-next": "13.4.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "7.32.2",
    "fetch-mock": "^9.11.0",
    "husky": "4.3.8",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "next-compose-plugins": "^2.2.1",
    "patch-package": "^7.0.0",
    "postcss": "^8.4.21",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "semantic-release": "^21.0.1",
    "svg-spreact": "^1.3.2",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.1.0",
    "tsc": "^2.0.4",
    "typescript": "5.0.4",
    "vitest": "^0.29.8"
  },
  "engines": {
    "node": ">=18.6.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mdx}": [
      "yarn format"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "packageManager": "yarn@1.22.19",
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/commit-analyzer",
      "@semantic-release/git",
      "@semantic-release/changelog"
    ]
  }
}
