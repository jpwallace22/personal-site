{
  "name": "next-enterprise",
  "version": "1.12.5",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "cross-env FORCE_COLOR=1 next dev --port 3030",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,mdx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx,mdx}\" --fix",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "format": "yarn lint:fix && yarn prettier:fix",
    "analyze": "cross-env ANALYZE=true yarn build",
    "test": "cross-env FORCE_COLOR=1 jest --passWithNoTests --silent",
    "types:check": "tsc --noEmit",
    "postinstall": "npx patch-package -y",
    "preinstall": "npx only-allow yarn",
    "gen:types": "DOTENV_CONFIG_PATH=.env graphql-codegen -r dotenv/config",
    "gen:icons": "node lib/gen-sprite.mjs -s src/assets/icons -o public/sprite.svg"
  },
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@apollo/experimental-nextjs-app-support": "^0.1.0",
    "@radix-ui/react-accordion": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.4",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-label": "^2.0.1",
    "@radix-ui/react-popover": "^1.0.5",
    "@radix-ui/react-radio-group": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.0.3",
    "@radix-ui/react-select": "1.2.2",
    "@radix-ui/react-slider": "^1.1.1",
    "@radix-ui/react-switch": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.5",
    "class-variance-authority": "^0.7.0",
    "datocms-structured-text-to-plain-text": "^2.0.4",
    "framer-motion": "^10.12.16",
    "graphql": "^16.6.0",
    "jest-axe": "^8.0.0",
    "next": "^13.4.7",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-datocms": "^4.1.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.32.1",
    "tailwind-merge": "^1.10.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@graphql-codegen/cli": "^4.0.0",
    "@graphql-codegen/client-preset": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-graphql-request": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@jest/globals": "^29.5.0",
    "@next/bundle-analyzer": "^13.4.12",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.4",
    "@semantic-release/npm": "^10.0.6",
    "@semantic-release/release-notes-generator": "^11.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@total-typescript/ts-reset": "^0.4.2",
    "@turbo/gen": "^1.10.6",
    "@types/gtag.js": "^0.0.12",
    "@types/jest-axe": "^3.5.5",
    "@types/node": "^20.5.6",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "eslint": "^8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "7.32.2",
    "fetch-mock": "^9.11.0",
    "husky": "4.3.8",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "next-compose-plugins": "^2.2.1",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.28",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "semantic-release": "^21.0.7",
    "svg-spreact": "^1.3.2",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=18.6.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mdx}": [
      "yarn format"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "turbo types:check && turbo test && turbo build"
    }
  },
  "packageManager": "yarn@1.22.19",
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/commit-analyzer",
      "@semantic-release/git",
      "@semantic-release/changelog"
    ]
  }
}
